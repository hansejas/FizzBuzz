<?xml version="1.0" encoding="utf-8" ?>
<configuration>

  <!--Create a user defined section to store tokens-->
  <!--DO NOT MODIFY THIS SECTION-->
  <configSections>
    <sectionGroup name="tokenInfo">
      <section name="tokenSection" type="System.Configuration.NameValueSectionHandler"/>
    </sectionGroup>
  </configSections>

  <!--Tokens are stored here.  Key must be a numeric value.-->
  <tokenInfo>
    <tokenSection>
      <!--Modifications can be performed here-->
      <add key="3" value="Fizz" />
      <add key="5" value="Buzz" />
    </tokenSection>
  </tokenInfo>

  <appSettings>
    <!--Modifications can be performed here-->
    <!--Application loops through Min and Max values to determine display of tokens.  Max must be larger value than min and int only.-->
    <add key="FormatterMin" value="1"/>
    <add key="FormatterMax" value="100"/>
    <!--Ouptut value should contain either Console or Window-->
    <add key="Output" value="Console"/>
  </appSettings>

</configuration>